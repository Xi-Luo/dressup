.title{
  font-size: 20rpx;
  font: 2em sans-serif;
  text-align: center;
  padding: 10rpx;
  width: 100%;
}
.sameSize{
  margin: 0rpx 0rpx 0rpx 200rpx;
  width: 400rpx;
  height: 600rpx;
}
.thudernail{
  width: 100rpx;
  height: 150rpx;
}
.malehair{
  margin: 0rpx 0rpx 0rpx 200rpx;
  width: 400rpx;
  height: 550rpx;
  position: absolute;
  z-index: 2;
  left: 0;
}

.eye{
  margin: 150rpx 0rpx 0rpx 310rpx;
  /* 上右下左 */
  width: 180rpx;
  height: 300rpx;
  position: absolute;
  z-index: 3;
  left: 0;
}

.glass{
  margin: 170rpx 0rpx 0rpx 275rpx;
  width: 250rpx;
  height: 290rpx;
  position: absolute;
  z-index: 4;
  left: 0;
}
.cloth{
  margin: 145rpx 0rpx 0rpx 200rpx;
  width: 400rpx;
  height: 400rpx;
  position: absolute;
  z-index: 1;
  left: 0;
}

.mouth{
  width: 120rpx;
  height: 200rpx;
  margin: 128px 0px 0px 145px;
  position: absolute;
  z-index: 3;
  left: 0;
}

page{
  background: #fff;
}
.top-position{
  border-radius: 200rpx;
  position: relative;
  top: -10rpx;
  right: -10rpx;
  padding: 0rpx 10rpx;
  height: 28rpx;
  color: var(--white);
  margin-top: -210rpx;
}
.boder-raduis{
  position: fixed;
  width: 70rpx;
  z-index: 2;
  height: 70rpx;
  border-radius: 50%;
  line-height: 70rpx;
  font-size: 50rpx;
  right: 10%;
  bottom: 5%;
  margin: auto;
  padding: 0;
}